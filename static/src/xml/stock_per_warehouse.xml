<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

	<t t-name="StockPerWhControlPanel">
		<div class="o_stock_per_wh_cp">
			<table class="o_stock_per_wh_table">
				<tbody>
					<tr>
						<td class="o_filter_label">Warehouse</td>
						<td class="o_filter_warehouse"></td>

						<td class="o_filter_label">Location</td>
						<td class="o_filter_location"></td>

						<td class="o_filter_label">Brand Name</td>
						<td class="o_filter_brand"></td>

						<td colspan="5"/>
					</tr>
					<tr>

						<td class="o_filter_label">Product Category</td>
						<td class="o_filter_product_category"></td>

						<td class="o_filter_label">Product Code</td>
						<td class="o_filter_product_code"></td>

						<td class="o_filter_label">Product Name</td>
						<td class="o_filter_product_name"></td>

						<td class="o_filter_label">Lot/Serial Number</td>
						<td class="o_filter_lot">
						</td>
					</tr>
					<tr>
						<td class="o_filter_label">Calculate Sold Product</td>
						<td class="o_filter_sold_product"></td>
						
						<td class="o_filter_label">Show Minus Stock Only</td>
						<td class="o_filter_minus_stock"></td>

						<td class="o_filter_label">Activate FSN Coloring</td>
						<td class="o_filter_fsn_color"></td>
						<td colspan="6" class="text-right">
							<div class="d-flex justify-content-end">
								<div class="o_filter_btn_apply ml-auto mr-2">
									
								</div>
								<div class="o_filter_btn_reset">
									
								</div>
							</div>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
	</t>

</templates>
